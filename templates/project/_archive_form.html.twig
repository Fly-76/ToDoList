<form method="post" action="{{ path('project_archiv', {'id': project.id}) }}" onsubmit="return confirm('Êtes vous sûr de vouloir archiver ce projet?');">
    <input type="hidden" name="_method" value="ARCHIV">
    <input type="hidden" name="_token" value="{{ csrf_token('archiv' ~ project.id) }}">
    <button class="btn">Archiver</button>
</form>
